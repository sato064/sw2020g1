@startuml



class User{
    name: string
    id: int
    login_id: string
    password: string

    }

    

class Project{
    title: string
    overview: string
    host_id: int
    deadline: datatime
    Status: int
    isDelayed: boolean
}

class Task{
    title: string
    overview: string
    deadline: datatime
    Status: int
